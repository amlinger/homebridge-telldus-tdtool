{
  "name": "homebridge-telldus-tdtool",
  "version": "1.0.3",
  "description": "A Telldus TDtool Adapter for Homebridge",
  "main": "./dist/index.js",
  "scripts": {
    "rsync": "fswatch -0 -o src |  xargs -0 -n 1 -I {} ./sync.sh",
    "test": "node_modules/.bin/jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build"
  },
  "keywords": [
    "telldus",
    "tdtool",
    "homebridge",
    "homebridge-plugin"
  ],
  "author": "Anton Amlinger <anton.amlinger@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "proxyquire": "^1.7.4",
    "jasmine": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/amlinger/homebridge-telldus-tdtool.git"
  },
  "dependencies": {
    "tellstick.conf-parser": "^1.0.1"
  },
  "engines": {
    "node": ">=0.12.0",
    "homebridge": ">=0.2.5"
  }
}
